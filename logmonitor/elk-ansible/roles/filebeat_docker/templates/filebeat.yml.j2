filebeat.inputs:
  {{ filebeat_inputs | to_json }}

processors:
  - add_docker_metadata:
      host: "unix:///var/run/docker.sock"

  - decode_json_fields:
      fields: ["message"]
      target: "json"
      overwrite_keys: true

output.logstash:
  hosts: {{ filebeat_output_logstash_hosts | to_json }}
  protocol: tcp

logging.json: true
logging.metrics.enabled: false
